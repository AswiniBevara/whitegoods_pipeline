            ],
            "properties": {
                "mode": "Incremental",
                "templateLink": {
                    "uri": "[concat(variables('baseUrl'), 'nested1/traffic-manager.json?token=AT3nZuldasRQo_bgfkob_a-QIYidoT25ks5ba-wiwA%3D%3D')]",
                    "contentVersion": "1.0.0.0"
                },
                "parameters": {
                    "webSiteSettings": {
