            "type": "Microsoft.Resources/deployments",
            "apiVersion": "2016-02-01",
            "dependsOn": [],
            "properties": {
                "mode": "Incremental",
                "templateLink": {
                    "uri": "[concat(variables('baseUrl'), 'nested1/app-insights.json?token=AT3nZmG4_9zwmf4vyJ2dulgXn4Pi0j8yks5basIIwA%3D%3D')]",
                    "contentVersion": "1.0.0.0"
