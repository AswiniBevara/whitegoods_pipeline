                    "uri": "[concat(variables('baseUrl'), 'nested1/timeseriesinsights.json?token=AT3nZo5gbTND4lpE17GgksJV-ycshd4cks5basK5wA%3D%3D')]",
                    "contentVersion": "1.0.0.0"
                },
                "parameters": {
                    "timeSeriesInsights": {
                        "value": "[variables('timeSeriesInsights')]"
                    },
