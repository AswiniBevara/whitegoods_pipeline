{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "webSiteSettings": {
            "type": "object"
        },
        "apiVersions": {
            "type": "object"
        },
        "iotHubEndPoint": {
            "type": "string"
        },
        "iotHubPrimaryKey": {
            "type": "string"
        },
        "cosmosDBAccountEndPoint": {
            "type": "string"
        },
        "cosmosDBAccountKey": {
            "type": "string"
        },
        "notificationHubConnectionString": {
            "type": "string"
        },
        "omsSettings": {
            "type": "object"
        }
    },
    "variables": {},
    "resources": [
        {
            "apiVersion": "[parameters('apiVersions').appInsightsApiVersion]",
            "name": "[parameters('webSiteSettings').dataPacketAppInsightsDr]",
            "type": "Microsoft.Insights/components",
            "location": "[parameters('webSiteSettings').appInsightslocation]",
            "tags": {
                "[concat('hidden-link:', resourceGroup().id, '/providers/Microsoft.Web/sites/', parameters('webSiteSettings').dataPacketWebSiteNameDr)]": "Resource",
                "displayName": "AppInsightsComponent"
            },
            "properties": {
                "applicationId": "[parameters('webSiteSettings').dataPacketAppInsightsDr]"
            }
        },
        {
            "apiVersion": "[parameters('apiVersions').appInsightsApiVersion]",
            "name": "[parameters('webSiteSettings').deviceManagementAppInsightsDr]",
            "type": "Microsoft.Insights/components",
            "location": "[parameters('webSiteSettings').appInsightslocation]",
            "tags": {
                "[concat('hidden-link:', resourceGroup().id, '/providers/Microsoft.Web/sites/', parameters('webSiteSettings').dataPacketWebSiteNameDr)]": "Resource",
                "displayName": "AppInsightsComponent"
            },
            "properties": {
                "applicationId": "parameters('webSiteSettings').deviceManagementAppInsightsDr]"
            }
        },
        {
            "apiVersion": "[parameters('apiVersions').webAppApiVersion]",
            "name": "[parameters('webSiteSettings').webAppHostingPlanNameDr]",
            "type": "Microsoft.Web/serverfarms",
            "location": "[parameters('webSiteSettings').locationDr]",
            "tags": {
                "displayName": "HostingPlan"
            },
            "sku": {
                "name": "[parameters('webSiteSettings').webAppSkuName]",
                "capacity": "1"
            },
            "properties": {
                "name": "[parameters('webSiteSettings').webAppHostingPlanNameDr]"
            }
        },
        {
            "apiVersion": "[parameters('apiVersions').webAppApiVersion]",
            "name": "[parameters('webSiteSettings').dataPacketWebSiteNameDr]",
            "type": "Microsoft.Web/sites",
            "location": "[parameters('webSiteSettings').locationDr]",
            "tags": {
                "[concat('hidden-related:', resourceGroup().id, '/providers/Microsoft.Web/serverfarms/', parameters('webSiteSettings').webAppHostingPlanNameDr)]": "Resource",
                "displayName": "Website"
            },
            "dependsOn": [
                "[resourceId('Microsoft.Web/serverfarms/', parameters('webSiteSettings').webAppHostingPlanNameDr)]",
                "[resourceId('microsoft.insights/components/', parameters('webSiteSettings').dataPacketAppInsights)]"
            ],
            "properties": {
                "name": "[parameters('webSiteSettings').dataPacketWebSiteNameDr]",
                "serverFarmId": "[resourceId('Microsoft.Web/serverfarms', parameters('webSiteSettings').webAppHostingPlanNameDr)]"
            },
            "resources": [
                {
                    "name": "MSDeploy",
                    "type": "extensions",
                    "location": "[parameters('webSiteSettings').locationDr]",
                    "apiVersion": "[parameters('apiVersions').webAppApiVersion]",
                    "dependsOn": [
                        "[resourceId('Microsoft.Web/sites', parameters('webSiteSettings').dataPacketWebSiteNameDr)]"
                    ],
                    "tags": {
                        "displayName": "WebPackageDeploy"
                    },
                    "properties": {
                        "packageUri": "[parameters('webSiteSettings').dataPacketPackageuri]",
                        "dbType": "None",
                        "connectionString": "",
                        "setParameters": {
                            "IIS Web Application Name": "[parameters('webSiteSettings').dataPacketWebSiteNameDr]"
                        }
                    }
                },
                {
                    "apiVersion": "[parameters('apiVersions').webAppApiVersion]",
                    "name": "Appsettings",
                    "type": "config",
                    "dependsOn": [
                        "[resourceId('Microsoft.Web/Sites', parameters('webSiteSettings').dataPacketWebSiteNameDr)]",
                        "[concat('Microsoft.Web/Sites/', parameters('webSiteSettings').dataPacketWebSiteNameDr, '/Extensions/MSDeploy')]"
                    ],
                    "properties": {
                        "APPINSIGHTS_INSTRUMENTATIONKEY": "[reference(concat('microsoft.insights/components/', parameters('webSiteSettings').dataPacketAppInsightsDr)).InstrumentationKey]",
                        "AzureAd:ClientId": "[parameters('webSiteSettings').clientId]",
                        "AzureAd:ClientSecret": "[parameters('webSiteSettings').clientSecret]",
                        "AzureAd:TenantId": "[parameters('webSiteSettings').tenantId]",
                        "AzureAd:CallbackPath": "/signin-oidc",
                        "IotHub:HostName": "[parameters('iotHubEndPoint')]",
                        "IotHub:ConnectionString": "[concat('HostName=', parameters('iotHubEndPoint'), ';SharedAccessKeyName=', parameters('webSiteSettings').iotHubKeyName, ';SharedAccessKey=', parameters('iotHubPrimaryKey'))]",
                        "CosmosDb:Endpoint": "[parameters('cosmosDBAccountEndPoint')]",
                        "CosmosDb:AuthKey": "[parameters('cosmosDBAccountKey')]",
                        "CosmosDb:Database": "[parameters('webSiteSettings').cosmosDbName]",
                        "CosmosDb:Collections:Templates": "[parameters('webSiteSettings').cosmosDbColTemplateName]",
                        "CosmosDb:Collections:Messages": "[parameters('webSiteSettings').cosmosDbColMessagesName]",
                        "CosmosDb:Collections:Groups": "[parameters('webSiteSettings').cosmosDbColGroupsName]",
                        "CosmosDb:TemplatesBase": "https://msiotsolutiondev.blob.core.windows.net/cosmosdbtemplates/"
                    }
                },
                {
                    "name": "Microsoft.Insights/service",
                    "type": "providers/diagnosticSettings",
                    "apiVersion": "2015-07-01",
                    "dependsOn": [
                        "[concat('Microsoft.Web/sites/', parameters('webSiteSettings').dataPacketWebSiteNameDr)]",
                        "[concat('Microsoft.Web/sites/', parameters('webSiteSettings').dataPacketWebSiteNameDr, '/extensions/MSDeploy')]"
                    ],
                    "properties": {
                        "workspaceId": "[resourceId('Microsoft.OperationalInsights/workspaces/', parameters('omsSettings').omsWorkspaceName)]",
                        "metrics": [
                            {
                                "timeGrain": "PT1M",
                                "enabled": true,
                                "retentionPolicy": {
                                    "enabled": false,
                                    "days": 0
                                }
                            }
                        ]
                    }
                }
            ]
        },
        {
            "apiVersion": "[parameters('apiVersions').webAppApiVersion]",
            "name": "[parameters('webSiteSettings').deviceManagementPortalWebSiteNameDr]",
            "type": "Microsoft.Web/sites",
            "location": "[parameters('webSiteSettings').locationDr]",
            "tags": {
                "[concat('hidden-related:', resourceGroup().id, '/providers/Microsoft.Web/serverfarms/', parameters('webSiteSettings').webAppHostingPlanNameDr)]": "Resource",
                "displayName": "Website"
            },
            "dependsOn": [
                "[resourceId('Microsoft.Web/serverfarms/', parameters('webSiteSettings').webAppHostingPlanNameDr)]",
                "[resourceId('microsoft.insights/components/', parameters('webSiteSettings').deviceManagementAppInsights)]"
            ],
            "properties": {
                "name": "[parameters('webSiteSettings').deviceManagementPortalWebSiteNameDr]",
                "serverFarmId": "[resourceId('Microsoft.Web/serverfarms', parameters('webSiteSettings').webAppHostingPlanNameDr)]"
            },
            "resources": [
                {
                    "name": "MSDeploy",
                    "type": "extensions",
                    "location": "[parameters('webSiteSettings').locationDr]",
                    "apiVersion": "[parameters('apiVersions').webAppApiVersion]",
                    "dependsOn": [
                        "[resourceId('Microsoft.Web/sites', parameters('webSiteSettings').deviceManagementPortalWebSiteNameDr)]"
                    ],
                    "tags": {
                        "displayName": "WebPackageDeploy"
                    },
                    "properties": {
                        "packageUri": "[parameters('webSiteSettings').deviceManagementPortalPackageuri]",
                        "dbType": "None",
                        "connectionString": "",
                        "setParameters": {
                            "IIS Web Application Name": "[parameters('webSiteSettings').deviceManagementPortalWebSiteNameDr]"
                        }
                    }
                },
                {
                    "apiVersion": "[parameters('apiVersions').webAppApiVersion]",
                    "name": "Appsettings",
                    "type": "config",
                    "dependsOn": [
                        "[resourceId('Microsoft.Web/Sites', parameters('webSiteSettings').deviceManagementPortalWebSiteNameDr)]",
                        "[concat('Microsoft.Web/Sites/', parameters('webSiteSettings').deviceManagementPortalWebSiteNameDr, '/Extensions/MSDeploy')]"
                    ],
                    "properties": {
                        "APPINSIGHTS_INSTRUMENTATIONKEY": "[reference(concat('microsoft.insights/components/', parameters('webSiteSettings').deviceManagementAppInsightsDr)).InstrumentationKey]",
                        "AzureAd:ClientId": "[parameters('webSiteSettings').clientId]",
                        "AzureAd:ClientSecret": "[parameters('webSiteSettings').clientSecret]",
                        "AzureAd:TenantId": "[parameters('webSiteSettings').tenantId]",
                        "AzureAd:CallbackPath": "/signin-oidc",
                        "IotHub:HostName": "[parameters('iotHubEndPoint')]",
                        "IotHub:ConnectionString": "[concat('HostName=', parameters('iotHubEndPoint'), ';SharedAccessKeyName=', parameters('webSiteSettings').iotHubKeyName, ';SharedAccessKey=', parameters('iotHubPrimaryKey'))]",
                        "IotHub:CacheRefreshMinutes": "1",
                        "CosmosDb:Endpoint": "[parameters('cosmosDBAccountEndPoint')]",
                        "CosmosDb:AuthKey": "[parameters('cosmosDBAccountKey')]",
                        "CosmosDb:Database": "[parameters('webSiteSettings').cosmosDbName]",
                        "NotificationHub:ConnectionString": "[parameters('notificationHubConnectionString')]"
                    }
                },
                {
                    "name": "Microsoft.Insights/service",
                    "type": "providers/diagnosticSettings",
                    "apiVersion": "2015-07-01",
                    "dependsOn": [
                        "[concat('Microsoft.Web/sites/', parameters('webSiteSettings').deviceManagementPortalWebSiteNameDr)]",
                        "[concat('Microsoft.Web/sites/', parameters('webSiteSettings').deviceManagementPortalWebSiteNameDr, '/extensions/MSDeploy')]"
                    ],
                    "properties": {
                        "workspaceId": "[resourceId('Microsoft.OperationalInsights/workspaces/', parameters('omsSettings').omsWorkspaceName)]",
                        "metrics": [
                            {
                                "timeGrain": "PT1M",
                                "enabled": true,
                                "retentionPolicy": {
                                    "enabled": false,
                                    "days": 0
                                }
                            }
                        ]
                    }
                }
            ]
        }
    ],
    "outputs": {
        "dataPacketDesignerUrl": {
            "type": "string",
            "value": "[concat('https://', parameters('webSiteSettings').dataPacketWebSiteNameDr,'.azurewebsites.net')]"
        },
        "deviceManagementPortalUrl": {
            "type": "string",
            "value": "[concat('https://', parameters('webSiteSettings').deviceManagementPortalWebSiteNameDr,'.azurewebsites.net')]"
        }
    }
}
